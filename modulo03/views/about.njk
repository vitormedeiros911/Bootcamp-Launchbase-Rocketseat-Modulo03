{% extends "layout.njk" %}

{% block head %}
    <title>Sobre Vitor Eduardo</title>
{% endblock %}

{% block content %}

    <div id="wrapper">
        <img src="{{about.avatarUrl}}" alt="Vitor">
        <h1>
            {{about.name}}
        </h1>
        <h2>{{about.role}}</h2>
        <p>{{about.description}}</p>
    </div>
    <div id="links_footer" class="links">
        {%for link in about.links%}
            <a href="{{link.url}}" target="_blank">{{link.name}}</a>
        {% endfor %}
    </div>
{% endblock %}